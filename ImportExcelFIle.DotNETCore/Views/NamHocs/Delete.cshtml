@model ImportExcelFIle.DotNETCore.Models.NamHoc

@{
    ViewData["Title"] = "Delete";
}

<h1>Xóa năm học</h1>

<h3>Xác nhận xóa năm học?</h3>
<div>

    <div asp-validation-summary="ModelOnly" class="text-danger"></div>

    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.TenNamHoc)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TenNamHoc)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="IdNamHoc" />
        <input type="submit" value="Xóa năm học" class="btn btn-danger" /> |
        <a class="btn btn-info" asp-controller="Trainning" asp-action="Index" asp-route-Tab="NamHocsTab">Quay lại</a>
    </form>
</div>
